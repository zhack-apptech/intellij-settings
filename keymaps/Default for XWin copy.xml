<keymap version="1" name="Default for XWin copy" parent="Default for XWin">
  <action id="ActivateRunDashboardToolWindow">
    <keyboard-shortcut first-keystroke="alt 8" />
  </action>
  <action id="EditorAddOrRemoveCaret">
    <mouse-shortcut keystroke="alt button1" />
  </action>
  <action id="Educational.Check">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" second-keystroke="c" />
  </action>
  <action id="Educational.NextTask">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" second-keystroke="n" />
  </action>
  <action id="Hyperskill.PeekSolution">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" second-keystroke="p" />
  </action>
  <action id="Macro.Switch Projects To Development">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="Macro.Switch Projects To Production">
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="ctrl s" second-keystroke="ctrl h" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl s" second-keystroke="ctrl v" />
  </action>
  <action id="UnsplitAll">
    <keyboard-shortcut first-keystroke="ctrl u" second-keystroke="ctrl a" />
  </action>
</keymap>